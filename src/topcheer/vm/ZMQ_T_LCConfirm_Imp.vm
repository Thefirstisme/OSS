<?xml version="1.0" encoding="GBK"?>
<CFX>
	<HEAD>
		<VER>$!head.ver</VER>
		<SRC>$!head.src</SRC>
		<DES>$!head.des</DES>
		<APP>$!head.app</APP>
		<MsgNo>$!head.msgNo</MsgNo>
		<MsgID>$!head.msgID</MsgID>
		<MsgRef>$!head.msgRef</MsgRef>
		<WorkDate>$!head.workDate</WorkDate>
		<EditFlag>$!head.editFlag</EditFlag>
		<Reserve/>
	</HEAD>
	<MSG>
		#foreach($batch in ${batchList})
		<FTZ210304>
			<ReportCode>$!batch.reportId</ReportCode>
			<SeqNo>$!batch.declareSeq</SeqNo>
			<SubmitDate>$!batch.declareDt</SubmitDate>
			<AccOrgCode>$!batch.belongOrgId</AccOrgCode>
			<Amount>$!batch.bal</Amount>
			<Currency>$!batch.currencyCd</Currency>
			<OppSwiftCode>$!batch.abroadOppBankId</OppSwiftCode>
			<ApplicantEntName>$!batch.applicantCorpNm</ApplicantEntName>
			<ApplicantEntInstitutionCode>$!batch.applicantCorpOrgId</ApplicantEntInstitutionCode>
			<ApplicantCountryCode>$!batch.applicantCountryCd</ApplicantCountryCode>
			<ApplicantDistrictCode>$!batch.applicantDomeAreaCd</ApplicantDistrictCode>
			<ConfirmedExpireDate>$!batch.confirmMatureDt</ConfirmedExpireDate>
		</FTZ210304>
		#end
	</MSG>
</CFX>
